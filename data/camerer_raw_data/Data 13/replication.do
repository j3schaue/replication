/* Replication of Huck et al. (2011) AER    *//*                                          *//* Colin Camerer, Taisuke Imai, Gideon Nave *//* 10/09/2015                               *//*                                          */// Set memoryclearset mem 100m// Set working directorycd "SET WORKING DIRECTORY HERE"// Merge FCT and NCT data, keep the variable of interest W2+W3clearuse merged_fct.dtakeep SessionID Period Subject type toss1 toss2 sumpay23 agegen nct = 0replace type = type - 1keep if type == 0replace SessionID = 100 + SessionIDreplace Subject   = 1000 + Subjectorder SessionID nct Subject Period type toss1 toss2 sumpay23save temp1.dta, replaceclearuse merged_nct.dtakeep SessionID Period Subject type toss1 toss2 sumpay23 agegen nct = 1replace type = type - 1keep if type == 0 & toss1 == 0 & toss2 == 0replace SessionID = 200 + Sessionreplace Subject   = 2000 + Subjectorder Session nct Subject Period type toss1 toss2 sumpay23save temp2.dta, replaceclearuse temp1.dtaappend using temp2.dtasave pay23_all.dta, replaceerase temp1.dtaerase temp2.dta// Load merged Dataclearuse pay23_all.dtatabulate SessionID, gen(s)bysort nct: summarize sumpay23// Mann-Whitney Testpreservecollapse (mean) sumpay23, by(nct SessionID)bysort nct: summarize sumpay23kwallis sumpay23, by(nct)restore// Regressionregress sumpay23 nct, cluster(Subject)display 2*ttail(e(df_r),abs(_b[nct]/_se[nct]))